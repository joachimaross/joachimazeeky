# ==============================================
# ZEEKY AI - ENVIRONMENT CONFIGURATION
# ==============================================
# Copy this file to .env and fill in your actual API keys
# Never commit .env to version control!

# ==============================================
# FIREBASE CONFIGURATION
# ==============================================
# Get these from Firebase Console: https://console.firebase.google.com/
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id

# ==============================================
# AI SERVICE INTEGRATIONS
# ==============================================

# OpenAI API (for GPT-4 and advanced AI features)
# Get from: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=sk-your_openai_api_key_here

# Google Gemini API (for Google AI integration)
# Get from: https://makersuite.google.com/app/apikey
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Anthropic Claude API (for Claude AI integration)
# Get from: https://console.anthropic.com/
REACT_APP_CLAUDE_API_KEY=your_claude_api_key_here

# ==============================================
# MUSIC & MEDIA SERVICES
# ==============================================

# Suno AI API (for music generation)
# Get from: https://suno.ai/api
REACT_APP_SUNO_API_KEY=your_suno_api_key_here

# Spotify Web API (for music streaming)
# Get from: https://developer.spotify.com/dashboard/applications
REACT_APP_SPOTIFY_CLIENT_ID=your_spotify_client_id
REACT_APP_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# ==============================================
# EXTERNAL DATA SERVICES
# ==============================================

# OpenWeatherMap API (for weather data)
# Get from: https://openweathermap.org/api
REACT_APP_WEATHER_API_KEY=your_weather_api_key_here

# News API (for news feeds)
# Get from: https://newsapi.org/
REACT_APP_NEWS_API_KEY=your_news_api_key_here

# Google Maps API (for location services)
# Get from: https://developers.google.com/maps
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# ==============================================
# BUSINESS INTEGRATIONS
# ==============================================

# Zapier Webhook URL (for automation)
REACT_APP_ZAPIER_WEBHOOK_URL=your_zapier_webhook_url_here

# Twilio API (for SMS/call handling)
# Get from: https://console.twilio.com/
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token
REACT_APP_TWILIO_PHONE_NUMBER=your_twilio_phone_number

# ==============================================
# SOCIAL MEDIA APIS
# ==============================================

# Facebook/Instagram API
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id
REACT_APP_FACEBOOK_APP_SECRET=your_facebook_app_secret

# Twitter/X API
REACT_APP_TWITTER_API_KEY=your_twitter_api_key
REACT_APP_TWITTER_API_SECRET=your_twitter_api_secret

# TikTok API
REACT_APP_TIKTOK_CLIENT_KEY=your_tiktok_client_key
REACT_APP_TIKTOK_CLIENT_SECRET=your_tiktok_client_secret

# ==============================================
# DEVELOPMENT SETTINGS
# ==============================================

# Environment (development, staging, production)
NODE_ENV=development

# Debug mode
REACT_APP_DEBUG_MODE=true

# API Timeout (milliseconds)
REACT_APP_API_TIMEOUT=30000

# ==============================================
# SECURITY SETTINGS
# ==============================================

# JWT Secret for authentication
JWT_SECRET=your_super_secure_jwt_secret_here

# Encryption key for sensitive data
REACT_APP_ENCRYPTION_KEY=your_encryption_key_here

# ==============================================
# MONITORING & ANALYTICS
# ==============================================

# Google Analytics
REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Sentry (for error tracking)
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# ==============================================
# DEPLOYMENT SETTINGS
# ==============================================

# Production URL
REACT_APP_PRODUCTION_URL=https://zeeky.ai

# CDN URL (for assets)
REACT_APP_CDN_URL=https://cdn.zeeky.ai

# API Base URL
REACT_APP_API_BASE_URL=https://api.zeeky.ai