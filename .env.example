# ZEEKY AI ENVIRONMENT CONFIGURATION EXAMPLE
# =============================================
# Copy this file to .env and replace placeholder values with your actual API keys

# Environment Settings
NODE_ENV=development
REACT_APP_ENVIRONMENT=development
GENERATE_SOURCEMAP=true

# Firebase Configuration (Required)
# Get these from https://console.firebase.google.com/
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789012
REACT_APP_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890

# Firebase Emulator (Development only)
REACT_APP_USE_FIREBASE_EMULATOR=false

# AI Service APIs
# OpenAI (Required for main AI functionality)
REACT_APP_OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Google Gemini AI (Optional secondary AI)
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Claude AI (Optional third AI option)
REACT_APP_CLAUDE_API_KEY=sk-ant-api01-your_claude_api_key_here

# Music Generation Services
# Suno AI for music generation
REACT_APP_SUNO_API_KEY=your_suno_api_key_here
REACT_APP_SUNO_API_URL=https://api.suno.ai/v1

# Udio AI for music generation (alternative)
REACT_APP_UDIO_API_KEY=your_udio_api_key_here

# External Service APIs
# Weather data
REACT_APP_WEATHER_API_KEY=your_openweather_api_key_here

# News API
REACT_APP_NEWS_API_KEY=your_news_api_key_here

# Spotify (for music recommendations)
REACT_APP_SPOTIFY_CLIENT_ID=your_spotify_client_id
REACT_APP_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Voice and Speech Services
# ElevenLabs for voice synthesis
REACT_APP_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google Cloud Speech-to-Text
REACT_APP_GOOGLE_SPEECH_API_KEY=your_google_speech_api_key_here

# Business Integration
# Twilio for SMS
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token
REACT_APP_TWILIO_PHONE_NUMBER=your_twilio_phone_number

# SendGrid for email
REACT_APP_SENDGRID_API_KEY=your_sendgrid_api_key_here

# Google Calendar API
REACT_APP_GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key

# Analytics and Monitoring
# Google Analytics
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id_here

# Sentry for error tracking
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Application Settings
REACT_APP_APP_NAME="Zeeky AI"
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION="Advanced Emotionally Intelligent AI Assistant"

# Avatar and UI Settings
REACT_APP_AVATAR_ENABLED=true
REACT_APP_VOICE_ENABLED=true
REACT_APP_CAMERA_ENABLED=true
REACT_APP_MUSIC_ENABLED=true

# Security Settings (Production)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_DEBUG_MODE=true

# API Rate Limiting
REACT_APP_API_RATE_LIMIT=100
REACT_APP_CONVERSATION_HISTORY_LIMIT=50

# Custom Domain (for production)
REACT_APP_DOMAIN=your-domain.com
REACT_APP_API_BASE_URL=https://api.your-domain.com